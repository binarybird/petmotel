{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Tokens": {
    "PrivateKey": "private.xml",
    "PublicKey": "public.xml",
    "Lifetime": "86400",
    "Issuer": "http://localhost:5000",
    "Audience": "http://localhost:4000"
  },
  "RabbitMq": {
    "Uri": "petmotel-mq.petmotel.svc.cluster.local",
    "CertPath": "/tls/tls.crt",
    "KeyPath": "/tls/tls.key",
    "UserPath": "/rabbitmq/username",
    "PassPath": "/rabbitmq/password"
  },
  "IdentityUri": "petmotel-identity.petmotel.svc.cluster.local",
  "BasketUri": "petmotel-basket.petmotel.svc.cluster.local"
}
