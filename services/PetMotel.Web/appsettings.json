{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RabbitMq": {
    "Uri": "petmotel-mq.petmotel.svc.cluster.local",
    "CertPath": "/tls/tls.crt",
